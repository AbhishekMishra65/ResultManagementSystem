<div class="p-4">
    <h3 class="mt-3">Edit Result</h3>
    <form method="POST">
        <div class="mb-3 col-6">
            <label for="exampleInputEmail1" clas="form-label">Roll No</label>
            <input type="number" name="rollno" value="<%= student.rollno %>" id="exampleInputEmail1"
                class="form-control">
        </div>
        <div class="mb-3 col-6">
            <label for="exampleInputEmail1" clas="form-label">Name</label>
            <input type="text" name="name" value="<%= student.name %>" id="exampleInputEmail1" class="form-control">
        </div>
        <div class="mb-3 col-6">
            <label for="exampleInputEmail1" clas="form-label">Date of Birth</label>
            <input type="date" name="dob" value="<%= student.dob %>" id="exampleInputEmail1" class="form-control">
        </div>
        <div class="mb-3 col-6">
            <label for="exampleInputEmail1" clas="form-label">Score</label>
            <input type="number" name="score" value="<%= student.score %>" id="exampleInputEmail1" class="form-control">
        </div>
        <a class="btn btn-dark" href="/api/teacher/studentList">Back</a>
        <button type="submit" class="btn btn-primary">Update Result</button>
    </form>
</div>